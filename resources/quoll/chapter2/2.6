(define zero (lambda (f) (lambda (x) x)))

(define (add-1 n)
    (lambda (f) (lambda (x) (f ((n f) x)))))

(define one
 (lambda (f)
  (lambda (x) (f x))))

(define two
 (lambda (f)
  (lambda (x) (f (f x)))))

(define three
 (lambda (f)
  (lambda (x) (f (f (f x))))))

(define four
 (lambda (f)
  (lambda (x) (f (f (f (f x)))))))

(define (+ a b)
 (lambda (f)
   (lambda (x) ((b f) ((a f) x)))))

(define (* a b)
 (lambda (x) (b (a x))))
